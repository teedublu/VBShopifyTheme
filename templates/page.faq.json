    "faq_7DNJPa": {
      "type": "faq",
      "blocks": {
        "item_LBCKh7": {
          "type": "item",
          "settings": {
            "title": "What is the best way to charge my player?",
            "content": "<ul><li>use the USB C charging cable provided<\/li><li>charge using a USB adapter in a wall socket<\/li><li>allow the player to charge fully e.g. overnight<\/li><li>switch off the player when not in use<\/li><\/ul>"
          }
        },
        "item_NfcCGb": {
          "type": "item",
          "settings": {
            "title": "Can I charge my player using a laptop charger?",
            "content": "<p>No. Laptop chargers should be avoided. <\/p>"
          }
        },
        "item_GChHNj": {
          "type": "item",
          "settings": {
            "title": "Can I charge my player in the car?",
            "content": "<p>No. Car chargers should be avoided as they don't charge the player properly. <\/p>"
          }
        },
        "item_3mXn8F": {
          "type": "item",
          "settings": {
            "title": "Does the USB cable work when charging through a multi USB port?",
            "content": "<p>No. The best way is to plug the cable directly into a USB mains plug and into a wall socket. <\/p>"
          }
        }
      },
