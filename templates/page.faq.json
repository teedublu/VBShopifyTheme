    "faq_UE4NYB": {
      "type": "faq",
      "blocks": {
        "item_N94364": {
          "type": "item",
          "settings": {
            "title": "Can I recycle the Voxblock packaging?",
            "content": "<p>Absolutely, all our packaging is made of card and paper. No plastic here.<\/p>"
          }
        },
        "item_4c79Qg": {
          "type": "item",
          "settings": {
            "title": "How long will my Voxblock player last?",
            "content": "<p>Designed with the minimum number of component parts, our players are designed to be simple and straightforward, and live a long and happy life.<\/p>"
          }
        }
      },
