    "faq_UdwyVf": {
      "type": "faq",
      "blocks": {
        "item_KB36d6": {
          "type": "item",
          "settings": {
            "title": "What is Voxblock?",
            "content": "<p>Voxblock is a screen free audiobook player for kids. Our content is stored on physical blocks to be played in our player, with no need for apps or wifi.<\/p>"
          }
        },
        "item_idQyqH": {
          "type": "item",
          "settings": {
            "title": "Why is Voxblock screen free?",
            "content": "<p>We're not anti-screen - we know that phones and tablets can be great educational tools. But we wanted to create a way of listening to audiobooks that's simple, with the focus on the story alone.<\/p>"
          }
        },
        "item_gYbGJR": {
          "type": "item",
          "settings": {
            "title": "I can't find my question",
            "content": "<p><a href=\"\/pages\/voxblock-faq\" target=\"_blank\" title=\"FAQ : all about our screen free audiobook player\"><strong>See all FAQs<\/strong><\/a><\/p>"
          }
        }
