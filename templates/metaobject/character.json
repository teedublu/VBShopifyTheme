    "faq_PWkrrR": {
      "type": "faq",
      "blocks": {
        "item_dpCpac": {
          "type": "item",
          "settings": {
            "title": "What is Voxblock?",
            "content": "<p>A screen-free, wifi free, easy-to-use audiobook player, that makes learning and listening fun and engaging. Learn more <a href=\"\/pages\/how-does-it-work\" title=\"How it works\">here.<\/a><\/p>"
          }
        },
        "item_JwG9hG": {
          "type": "item",
          "settings": {
            "title": "What stories are available?",
            "content": "<p>We are focussing on delivering world-class stories for children aged 3 and up. We have over 200 titles and are regularly expanding and updating our array of content, so keep an eye on our website!<\/p>"
          }
        },
        "item_c3zFfM": {
          "type": "item",
          "settings": {
            "title": "What age is Voxblock suitable for?",
            "content": "<p>Our current range of content is aimed at toddlers to teenagers.  The player itself is suitable for all ages, with children from age two able to use Voxblock independently.<\/p>"
          }
        },
        "item_M9imEA": {
          "type": "item",
          "settings": {
            "title": "Any questions?",
            "content": "<p>You can contact us through our contact page! We will be happy to assist you.<br\/><a href=\"\/pages\/voxblock-faq\" title=\"FAQ : all about our screen free audiobook player\">More FAQ's here.<\/a><\/p>"
          }
        }
      },
